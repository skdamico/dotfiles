alias la="ls -la"

## git
alias g="git"
alias gs="git status"
alias gsu="git submodule sync && git submodule update --init --recursive"
alias gco="git checkout"
alias gp="git push -u origin HEAD"

alias f="ack -R --all-types --smart-case"

alias docker-cleanup="docker run -v /var/run/docker.sock:/var/run/docker.sock -v /var/lib/docker:/var/lib/docker --rm martin/docker-cleanup-volumes"
alias flush-redis="docker exec www_redis_1 redis-cli -c 'FLUSHALL'"
alias pg-start="pg_ctl -D /usr/local/var/postgres -l /usr/local/var/postgres/server.log start"
alias pg-stop="pg_ctl -D /usr/local/var/postgres stop -s -m fast"

## ngrok
alias ngrok-start="ngrok start -config=/Users/stef/.ngrok2/ngrok.yml -config=_ignore_this/ngrok_config dev webpack"

## vscode
alias code="/Applications/Visual\ Studio\ Code.app/Contents/Resources/app/bin/code"
